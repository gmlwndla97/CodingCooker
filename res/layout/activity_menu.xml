<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:sl="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gpsk1.kidscordinggame.menu">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/menu"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:id="@+id/hamburger"
        android:layout_width="280dp"
        android:layout_height="90dp"
        android:layout_above="@+id/mandu"
        android:layout_alignStart="@+id/mandu"
        android:layout_marginBottom="11dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:srcCompat="@drawable/hamburger" />

    <ImageButton
        android:id="@+id/mandu"
        android:layout_width="280dp"
        android:layout_height="90dp"

        android:layout_above="@+id/stick"
        android:layout_alignStart="@+id/stick"
        android:layout_marginBottom="11dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:srcCompat="@drawable/mandu" />


    <ImageButton
        android:id="@+id/stick"
        android:layout_width="280dp"
        android:layout_height="90dp"
        android:layout_above="@+id/home"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="31dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="visible"
        app:srcCompat="@drawable/stickgame" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="180dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/home" />

</RelativeLayout>
